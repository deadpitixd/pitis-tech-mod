{
  "multipart": [
    {
      "apply": { "model": "ptm:block/cable_core" }
    },
    {
      "when": { "north": "true" },
      "apply": { "model": "ptm:block/cable_extension"}
    },
    {
      "when": { "east": "true" },
      "apply": { "model": "ptm:block/cable_extension", "y": 90}
    },
    {
      "when": { "south": "true" },
      "apply": { "model": "ptm:block/cable_extension", "y": 180}
    },
    {
      "when": { "west": "true" },
      "apply": { "model": "ptm:block/cable_extension", "y": 270}
    },
    {
      "when": { "up": "true" },
      "apply": { "model": "ptm:block/cable_extension", "x": 270}
    },
    {
      "when": { "down": "true" },
      "apply": { "model": "ptm:block/cable_extension", "x": 90}
    }
  ]
}